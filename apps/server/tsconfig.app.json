{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "src/dist",
    "types": [
      "node"
    ],
    "rootDir": "src",
    "tsBuildInfoFile": "src/dist/tsconfig.app.tsbuildinfo",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "target": "es2021"
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "eslint.config.js",
    "eslint.config.cjs",
    "eslint.config.mjs"
  ]
}